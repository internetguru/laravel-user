{
    "name": "internetguru/laravel-user",
    "description": "Interent Guru Laravel User",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "InternetGuru\\LaravelUser\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
