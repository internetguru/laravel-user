{
    "name": "internetguru/laravel-user",
    "description": "Interent Guru Laravel User",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "InternetGuru\\LaravelUser\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "tests/src/App/",
            "Database\\": "tests/src/Database/"
        }
    },
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
        "laravel/socialite": "^5.16",
        "socialiteproviders/manager": "^4.6",
        "internetguru/laravel-common": "^0.12",
        "internetguru/laravel-model-browser": "^0.11"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "laravel/socialite": "^5.16",
        "orchestra/testbench": "^9.5",
        "phpunit/php-code-coverage": "^11.0",
        "socialiteproviders/manager": "^4.6",
        "livewire/livewire": "^3.5",
        "internetguru/laravel-common": "^0.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InternetGuru\\LaravelUser\\LaravelUserServiceProvider"
            ]
        }
    }
}
